{
    "name": "hfg/portfolios",
    "description": "Connect to Kirby Portfolios.",
    "type": "craft-plugin",
    "version": "1.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "portfolios"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Sonnenschein",
            "homepage": "https://niklassonnenschein.de"
        }
    ],
    "require": {
        "craftcms/cms": "^3.3.0",
        "flipboxfactory/craft-elements-nested-index": "^2.2.2"
    },
    "autoload": {
        "psr-4": {
          "hfg\\portfolios\\": "src/"
        }
    },
    "extra": {
        "name": "Portfolios",
        "handle": "portfolios",
        "hasCpSettings": true,
        "hasCpSection": false,
        "class": "hfg\\portfolios\\Portfolios"
    }
}
